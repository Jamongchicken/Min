import sys
def solution(numbers):
    numbers.sort()
    for i in range(len(numbers)-1):
        if numbers[i] in numbers[i+1]: 
            return False
    return True

numbers=[]
t=int(input())
answer=[]
for i in range(t):
    n=int(input())
    for _ in range(n):
        numbers.append(sys.stdin.readline().strip())
    answer.append(solution(numbers))
    numbers.clear()
for yn in answer:
    if yn == False:
        print('NO')
    else:
        print('YES')
